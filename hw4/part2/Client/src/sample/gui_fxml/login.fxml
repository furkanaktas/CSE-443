<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="600.0"
            prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="sample.LoginController">
    <children>
        <Button fx:id="loginButton" alignment="CENTER" layoutX="307.0" layoutY="294.0" mnemonicParsing="false"
                onAction="#logIn" prefHeight="31.0" prefWidth="188.0" text="Login"/>
        <TextField fx:id="username" alignment="CENTER" layoutX="307.0" layoutY="196.0" prefHeight="31.0"
                   prefWidth="188.0" promptText="Username (id)"/>
        <PasswordField fx:id="pass" alignment="CENTER" layoutX="307.0" layoutY="240.0" onAction="#logIn"
                       prefHeight="31.0" prefWidth="188.0" promptText="Password"/>
        <Button fx:id="registerButton" alignment="CENTER" layoutX="307.0" layoutY="339.0" mnemonicParsing="false"
                onAction="#register" prefHeight="31.0" prefWidth="188.0" text="Register System"/>
        <Label fx:id="loginStatus" layoutX="304.0" layoutY="154.0" prefHeight="31.0" prefWidth="188.0"/>

    </children>
</AnchorPane>
