<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.LoginController">

    <children>
        <Button fx:id="registerButton" alignment="CENTER" layoutX="206.0" layoutY="262.0" mnemonicParsing="false" onAction="#doRegister" prefHeight="31.0" prefWidth="188.0" text="Register" />
        <TextField fx:id="regname" alignment="CENTER" layoutX="90.0" layoutY="199.0" prefHeight="31.0" prefWidth="188.0" promptText="Name" />
        <PasswordField fx:id="regpass2" alignment="CENTER" layoutX="322.0" layoutY="148.0" prefHeight="31.0" prefWidth="188.0" promptText="Password (Again)" />
        <Label fx:id="regStatus" layoutX="206.0" layoutY="51.0" prefHeight="31.0" prefWidth="188.0" />
        <TextField fx:id="regsurname" alignment="CENTER" layoutX="322.0" layoutY="199.0" prefHeight="31.0" prefWidth="188.0" promptText="Surname" />
        <TextField fx:id="reguname" alignment="CENTER" layoutX="90.0" layoutY="98.0" prefHeight="31.0" prefWidth="188.0" promptText="Username (id)" />
        <PasswordField fx:id="regpass" alignment="CENTER" layoutX="90.0" layoutY="148.0" prefHeight="31.0" prefWidth="188.0" promptText="Password" />
      <TextField fx:id="cardNo" alignment="CENTER" layoutX="322.0" layoutY="98.0" prefHeight="31.0" prefWidth="188.0" promptText="Card No" />

    </children>

</AnchorPane>
